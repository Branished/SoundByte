<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters1="using:SoundByte.Core.Converters">
    
    <!-- Used throughout app to determine the highest quality artwork to 
         return (depending on the attached object -->
    <converters1:ArtworkConverter x:Key="ArtworkConverter" />
        
    <converters1:FormattedValueConverter x:Key="FormattedValueConverter" />

    <converters1:DateTimeConverter x:Key="ModernDateTimeConverter" />
    
    <converters1:TimeStampConverter x:Key="TimeStampConverter" />
</ResourceDictionary>
